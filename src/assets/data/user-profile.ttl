@prefix ff:  <https://foerderfunke.org/default#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

ff:this
    a ff:CitizenProfile .

ff:HouseA
    a           ff:House ;
    ff:roofArea 90 ;
    ff:houseAge 15 .

ff:CarA
    a          ff:Car ;
    ff:carAge  5 ;
    ff:carType "Electric" .

ff:citizen-a
    a                    ff:Person, ff:Citizen ;
    ff:owns              ff:HouseA ;
    ff:hasBirthday       "2001-01-15"^^xsd:date ;
    ff:hasResidence      "Berlin" ;
    ff:hasDrivingLicense true ;
    ff:owns              ff:CarA ;
    ff:hasChildren       2 .
