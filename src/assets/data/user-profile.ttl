@prefix fox: <https://foerderfox.org/default#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.

fox:this a fox:CitizenProfile .

fox:HouseA a fox:House ;
           fox:roofArea 90 ;
           fox:houseAge 15 .

fox:CarA a fox:Car ;
         fox:carAge 5 ;
         fox:carType "Electric" .

fox:citizen-a a fox:Person, fox:Citizen ;
              fox:owns fox:HouseA ;
              fox:hasBirthday "2001-01-15"^^xsd:date ;
              fox:hasResidence "Berlin" ;
              fox:hasDrivingLicense true ;
              fox:owns fox:CarA ;
              fox:hasChildren 2 .
